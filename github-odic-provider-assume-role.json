{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "sts:AssumeRole",
        "iam:CreatePolicy",
        "iam:AttachRolePolicy",
        "iam:CreateRole",
        "iam:UpdateAssumeRolePolicy",
        "iam:GetRole",
        "iam:PassRole"
      ],
      "Resource": [
        "arn:aws:iam::242201314218:role/GithubTrustPolicy",
        "arn:aws:iam::242201314218:policy/*"
      ]
    }
  ]
}